<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.avatye.sdk.example.adcash">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProjectAndroidCashBlock">

        <meta-data
            android:name="enable_ap_log"
            android:value="enable" />
        <meta-data
            android:name="cashblock.config.environment"
            android:value="live" />
        <meta-data
            android:name="cashblock.config.developer"
            android:value="true" />
        <meta-data
            android:name="cashblock.config.log"
            android:value="true" />

        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".banner.Banner320X50Activity"
            android:screenOrientation="portrait" />

        <activity
            android:name=".banner.BannerWidget320X50Activity"
            android:screenOrientation="portrait" />

        <activity
            android:name=".banner.Banner320X100Activity"
            android:screenOrientation="portrait" />

        <activity
            android:name=".banner.BannerWidget320X100Activity"
            android:screenOrientation="portrait" />

        <activity
            android:name=".banner.Banner300X250Activity"
            android:screenOrientation="portrait" />

        <activity
            android:name=".banner.BannerWidget300X250Activity"
            android:screenOrientation="portrait" />


        <activity
            android:name=".interstitial.InterstitialActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>